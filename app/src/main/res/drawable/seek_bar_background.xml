<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background" android:height="10dp"
        android:gravity="center">
        <shape>
            <corners android:radius="5dp"/>
            <solid android:color="#C1AAB2"/>
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress" android:height="10dp"
        android:gravity="center">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <solid android:color="#C1AAB2"/>            <!-- 颜色-->
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress" android:height="10dp"
        android:gravity="center">
        <clip>
            <shape>
                <corners android:radius="@dimen/y1"/>
                <solid android:color="#191718"/>   <!--  颜色-->
            </shape>
        </clip>
    </item>
</layer-list>